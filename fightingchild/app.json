{
  "pages": [
    "page/home/home",
    "page/userprofile/userprofile",
    "page/homecard/arithmetic/arithmetic",
    "page/homecard/literacy/literacy",
    "page/homecard/english/english",
    "page/homecard/wiki/wiki"
  ],
  "window": {
    "navigationBarTextStyle": "@navigationBarTextStyle",
    "navigationBarTitleText": "首页",
    "navigationBarBackgroundColor": "@navigationBarBackgroundColor",
    "backgroundColor": "@backgroundColor"
  },
  "tabBar": {
    "custom": false,
    "color": "@tabBarColor",
    "selectedColor": "@tabBarSelectedColor",
    "borderStyle": "@tabBarBorderStyle",
    "backgroundColor": "@tabBarBackgroundColor",
    "list": [
      {
        "pagePath": "page/home/home",
        "iconPath": "@tabbarHomeIcon",
        "selectedIconPath": "@tabbarHomeIconSelected",
        "text": "首页"
      },
      {
        "pagePath": "page/userprofile/userprofile",
        "iconPath": "@tabbarMeIcon",
        "selectedIconPath": "@tabbarMeIconSelected",
        "text": "我的"
      }
    ]
  },
  "networkTimeout": {
    "request": 10000,
    "connectSocket": 10000,
    "uploadFile": 10000,
    "downloadFile": 10000
  },
  "debug": false,
  "style": "v2",
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  },
  "cloud": true,
  "sitemapLocation": "sitemap.json",
  "themeLocation": "theme.json",
  "darkmode": true,
  "resizable": true,
  "useExtendedLib": {
    "weui": true
  },
  "frameset": true,
  "requiredBackgroundModes": [],
  "lazyCodeLoading": "requiredComponents"
}